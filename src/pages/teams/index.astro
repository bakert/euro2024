---
import Layout from "../../layouts/Layout.astro";
import Team from "../../components/Team.astro";
import { teams } from "../../data/teams.js";
---
<Layout title="Teams">
    <h1>Teams</h1>
    <ul>
        {Object.values(teams).map((team) => <li><Team name={team.name} /></li>)}
    </ul>
</Layout>
